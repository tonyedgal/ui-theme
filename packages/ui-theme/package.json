{
  "name": "@ui-theme/web",
  "version": "0.1.0",
  "description": "Cross-framework UI theme switching library",
  "type": "module",
  "keywords": [
    "theme",
    "dark-mode",
    "light-mode",
    "react",
    "vue",
    "angular",
    "svelte",
    "ui",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonyedgal/ui-theme.git"
  },
  "sideEffects": false,
  "exports": {
    "./core": {
      "import": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core/index.js"
      },
      "require": {
        "types": "./dist/core/index.d.cts",
        "default": "./dist/core/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/react/index.js"
      },
      "require": {
        "types": "./dist/react/index.d.cts",
        "default": "./dist/react/index.cjs"
      }
    },
    "./vue": {
      "import": {
        "types": "./dist/vue/index.d.ts",
        "default": "./dist/vue/index.js"
      },
      "require": {
        "types": "./dist/vue/index.d.cts",
        "default": "./dist/vue/index.cjs"
      }
    },
    "./angular": {
      "import": {
        "types": "./dist/angular/index.d.ts",
        "default": "./dist/angular/index.js"
      },
      "require": {
        "types": "./dist/angular/index.d.cts",
        "default": "./dist/angular/index.cjs"
      }
    },
    "./svelte": {
      "import": {
        "types": "./dist/svelte/index.d.ts",
        "default": "./dist/svelte/index.js"
      },
      "require": {
        "types": "./dist/svelte/index.d.cts",
        "default": "./dist/svelte/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/core/index.cjs",
  "module": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@radix-ui/react-select": ">=2.0.0",
    "vue": ">=3.3.0",
    "@angular/core": ">=18.0.0",
    "@angular/common": ">=18.0.0",
    "svelte": ">=4.0.0 || >=5.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "@radix-ui/react-select": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@radix-ui/react-select": "^2.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vue": "^3.5.0",
    "@angular/core": "^18.0.0",
    "@angular/common": "^18.0.0",
    "svelte": "^5.0.0"
  }
}
